@model string

@using (var container = Bootstrap.Container().AddCss("banner").Begin())
{
    using (var row = container.GridRow().Begin())
    {
        using (row.GridColumn().SetSm(12).Begin())
        { 
            @Bootstrap.Div().AddCss("title", Model).SetText(" " + ViewData[ViewDataKeys.Title])
        }
    }
    <hr />
}